{
  "name": "rick-morty-api-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "npx @nestjs /cli build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "deploy": "yarn run build && yarn run start:prod",
    "lint": "npx eslint .",
    "lint:fix": "yarn run lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "yarn run build && npx typeorm -d dist/database/data-source.js",
    "migration:generate": "yarn run typeorm -- migration:generate",
    "migration:run": "yarn run typeorm -- migration:run",
    "migration:revert": "yarn run typeorm -- migration:revert",
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d dist/database/data-source.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.254.0",
    "@aws-sdk/s3-request-presigner": "^3.254.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjsplus/redirect": "^1.0.0",
    "@types/aws-sdk": "^2.7.0",
    "aws-sdk": "^2.1298.0",
    "axios": "^1.2.2",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-typeorm": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "env-var": "^7.3.0",
    "express-session": "^1.17.3",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-discord": "^0.1.4",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-microsoft": "^1.0.0",
    "passport-spotify": "^2.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sharp": "^0.31.3",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11",
    "typeorm-extension": "^2.4.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/passport": "^1.0.11",
    "@types/passport-discord": "^0.1.6",
    "@types/passport-github2": "^1.2.5",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-spotify": "^2.0.1",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "*"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "moduleNameMapper": {
      "^@app(.*)$": "<rootDir>/src/app$1",
      "^@common(.*)$": "<rootDir>/src/common$1",
      "^@config(.*)$": "<rootDir>/src/config$1",
      "^@core(.*)$": "<rootDir>/src/core$1",
      "^@infrastructure(.*)$": "<rootDir>/src/infrastructure$1",
      "^@modules(.*)$": "<rootDir>/src/modules$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "coverageReporters": [
      "json",
      "html"
    ],
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "./dist",
      "./database",
      "./test/utils",
      "/src/modules",
      "/src/core",
      "/src/common/swagger",
      "/src/common/config",
      "/src/common/models",
      "./src/main.ts",
      "./src/app/app.module.ts"
    ]
  }
}
